{
  "manifest_version": 2,
  "name": "Chicago Food Truck Finder",
  "description": "Displays food trucks that are near you now or will be near you later",
  "version": "0.0.2",
  "background" : {
    "scripts": ["jquery-2.1.1.min.js","background.js", "popup.js", "options.js"]
  },
  "permissions": [
    "notifications",
    "geolocation",
    "storage",
    "http://www.chicagofoodtruckfinder.com/"
  ],
  "options_page" : "options.html",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}